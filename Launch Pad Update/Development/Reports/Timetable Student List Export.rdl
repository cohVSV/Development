  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>decvReportReader</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@enrolmentYear">
            <Value>=Parameters!enrolmentYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@semester">
            <Value>=Parameters!semester.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@subjectYearLevel">
            <Value>=Parameters!subjectYearLevel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@studentsOutsideOfSubjectYearLevel">
            <Value>=Parameters!studentsOutsideOfSubjectYearLevel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@subjectEnrolmentStatus">
            <Value>=Parameters!subjectEnrolmentStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@singleSubjectStudentsOnly">
            <Value>=Parameters!singleSubjectStudentsOnly.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@unallocatedStudentsOnly">
            <Value>=Parameters!unallocatedStudentsOnly.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @enrolmentYear int = 2021;
--declare @semester varchar(20) = '[B1]';
--declare @subjectYearLevel int = 8;
--declare @studentsOutsideOfSubjectYearLevel bit = 'true';
--declare @startDate date = '20210101';
--declare @endDate date = '20210316';
--declare @subjectEnrolmentStatus varchar(20) = '[PCD]';
--declare @unallocatedStudentsOnly bit = 'false';
--declare @singleSubjectStudentsOnly bit = 'false';

declare @dayAfterEndDate date = dateadd(day, 1, @endDate);
declare @enrolledStudents bit = iif(charindex('C', @subjectEnrolmentStatus) &lt;&gt; 0, 'true', 'false');
declare @singleSubjectSemester varchar(20) = iif(charindex('1', @semester) &lt;&gt; 0, '1', '2');


with
   StudentSemesterSubjectCount as
      (
         -- Inner joinable; will return 0 subjects if the student is not enrolled in any subjects (also likely be non enrolled at the school, with some exceptions)
         select
            st.STUDENT_ID,
            count(ss.STUDENT_ID) as SubjectCount
          from
            dbo.STUDENT as st
            inner join dbo.STUDENT_ENROLMENT as se
               on (se.STUDENT_ID = st.STUDENT_ID)
            left join dbo.STUDENT_SUBJECT as ss
               on (
                     -- We have to focus on enrolled subjects only.
                     -- There's currently no reliable way to detect "withdrawn" single subject students, as there may be subject changes
                     (ss.ENROLMENT_YEAR = se.ENROLMENT_YEAR)
                     and (ss.SEMESTER in ('B', @singleSubjectSemester))
                     and (ss.STUDENT_ID = se.STUDENT_ID)
                     and (ss.SUBJ_ENROL_STATUS in ('P', 'C', 'D'))
                     and (ss.SUBJECT_ID not in (select vis.IntroductorySubjectID from dbo.vwIntroductorySubject as vis where (vis.IntroductorySubjectStatus = 1)))
                  )
          where
            (se.ENROLMENT_YEAR = @enrolmentYear)
          group by
            st.STUDENT_ID
      )
 select
    st.STUDENT_ID,
    rtrim(st.SURNAME) as SURNAME,
    rtrim(st.FIRST_NAME) as FIRST_NAME,
    rtrim(st.PREFERRED_NAME) as PREFERRED_NAME,
    rtrim(st.MIDDLE_NAME) as MIDDLE_NAME,
    st.CATEGORY_CODE,
    st.SEX,
    null as roll_class,
    se.YEAR_LEVEL,
    ss.CLASS_CODE as home_group,
    rtrim(st.STUDENT_EMAIL) as STUDENT_EMAIL,
    sbj.SUBJECT_TITLE,
    ss.SUBJECT_ID,
    ss.ENROLMENT_DATE,
    ss.WITHDRAWAL_DATE
  from
    dbo.STUDENT as st
    inner join dbo.STUDENT_ENROLMENT as se
       on se.STUDENT_ID = st.STUDENT_ID
    inner join dbo.STUDENT_SUBJECT as ss
       on (
             (ss.STUDENT_ID = se.STUDENT_ID)
             and (ss.ENROLMENT_YEAR = se.ENROLMENT_YEAR)
          )
    inner join dbo.SUBJECT as sbj
       on sbj.SUBJECT_ID = ss.SUBJECT_ID
    inner join StudentSemesterSubjectCount as sssc
       on (sssc.STUDENT_ID = se.STUDENT_ID)
  where
    (se.ENROLMENT_YEAR = @enrolmentYear)
    and (ss.SEMESTER like (@semester + '%'))
    and (ss.SUBJECT_ID not in (select vis.IntroductorySubjectID from dbo.vwIntroductorySubject as vis where (vis.IntroductorySubjectStatus = 1)))
    and (sbj.YEAR_LEVEL in (@subjectYearLevel))
    and
    (
       (@studentsOutsideOfSubjectYearLevel = 'true')
       or (se.YEAR_LEVEL = sbj.YEAR_LEVEL)
    )
    and (ss.SUBJ_ENROL_STATUS like (@subjectEnrolmentStatus + '%'))
    and
    (
       (
          (@enrolledStudents = 'true')
          and (ss.ENROLMENT_DATE &gt;= @startDate)
          and (ss.ENROLMENT_DATE &lt; @dayAfterEndDate)
       )
       or
       (
          (@enrolledStudents = 'false')
          and (ss.WITHDRAWAL_DATE &gt;= @startDate)
          and (ss.WITHDRAWAL_DATE &lt; @dayAfterEndDate)
       )
    )
    and
    (
       (@unallocatedStudentsOnly = 'false')
       or (ss.STAFF_ID = 'N-A')
    )
    and
    (
       (@singleSubjectStudentsOnly = 'false')
       or (sssc.SubjectCount = 1)
    )
  order by
    iif(@enrolledStudents = 'true', ss.ENROLMENT_DATE, ss.WITHDRAWAL_DATE),
    st.SURNAME,
    st.FIRST_NAME;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
