    <DataSet Name="DefaultSemester">
      <Query>
        <DataSourceName>decvReportReader</DataSourceName>
        <CommandText>select iif(month(getdate()) &lt; 6, '[B1]', '[B2]') as DefaultSemester</CommandText>
      </Query>
      <Fields>
        <Field Name="DefaultSemester">
          <DataField>DefaultSemester</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DefaultStartDate">
      <Query>
        <DataSourceName>decvReportReader</DataSourceName>
        <CommandText>-- Select yesterday if today is between Tue - Sat, otherwise select the previous Friday
declare @theDate datetime = getdate();-- '20210315';
declare @dayOfWeek int = datepart(weekday, @theDate);

select
   iif(@dayOfWeek between 3 and 7,
      cast(dateadd(day, -1, @theDate) as date),
      cast(dateadd(day, - (@dayOfWeek + 1), @theDate) as date)) as DefaultStartDate;</CommandText>
      </Query>
      <Fields>
        <Field Name="DefaultStartDate">
          <DataField>DefaultStartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DefaultEndDate">
      <Query>
        <DataSourceName>decvReportReader</DataSourceName>
        <CommandText>-- Select yesterday

select cast(dateadd(day, -1, getdate()) as date) as DefaultEndDate;</CommandText>
      </Query>
      <Fields>
        <Field Name="DefaultEndDate">
          <DataField>DefaultEndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
