<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Creates CSV file of recently enrolled students (last 3 weeks, Years 3-12) to load to OnDemand testing. Admined by Terri R.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="decvReportReader">
      <DataSourceReference>/decvReportReader</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e48a154b-09a3-4919-9f80-9aad33793bea</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>decvReportReader</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromEnrolmentDate">
            <Value>=Parameters!FromEnrolmentDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @FromEnrolmentDate as date
--set @FromEnrolmentDate = '2016-08-1'

select distinct top 5000
--Student details
se.STUDENT_ID as STUDENT_CODE
, RTRIM(st.first_name) as FIRST_NAME
, isnull(rtrim(ST.MIDDLE_NAME),'') as MIDDLE_NAME
, rtrim(ST.SURNAME) as SURNAME
-- , ST.SEX 
, (case when ST.SEX='F' then 'FEMAL' else 'MALE' end) as GENDER
, convert(varchar(12), ST.BIRTH_DATE, 103) as DATE_OF_BIRTH	-- change to dd/mm/yyyy format as MMM dd yyyy format adds a comma
-- census stuff
, (case when (SC.FIRST_HOME_LANGUAGE &lt;&gt; '1201' or SC.OTHER_HOME_LANGUAGE is not null) then '1' else '0' end) as [LBOTE]
, (case when (SC.KOORITORRESFLAG = 'N') then '0' else '1' end) as [ATSI]
, (case when (SC.DISABILITY_CODES &lt;&gt; '0') then '1' else '0' end) as [Disability]
, (case when (SC.ALLOWANCE_CODE = '4') then '1' else '0' end) as [EMA]
, (case when (SC.FIRST_HOME_LANGUAGE &lt;&gt; '1201' and SC.OTHER_HOME_LANGUAGE ='1201') then '1' else '0' end) as [ESL]
, '' as HOME_GROUP	-- needed to be something even if not used
, SE.YEAR_LEVEL as YEAR_LEVEL
, SE.enrolment_date
from STUDENT_ENROLMENT SE, STUDENT ST, STUDENT_CENSUS_DATA SC, student_subject SS
where se.STUDENT_ID = st.STUDENT_ID and SE.STUDENT_ID = SC.STUDENT_ID
and se.STUDENT_ID = ss.STUDENT_ID and SE.enrolment_year = SS.enrolment_year and ss.subject_id in (select vis.IntroductorySubjectID from dbo.vwIntroductorySubject as vis where (vis.IntroductorySubjectStatus = 1) and (vis.IntroductorySubjectYearLevel between 0 and 10))
and SE.ENROLMENT_YEAR = year(getdate())
and SE.YEAR_LEVEL between 3 and 12	-- change as needed - is primarily for LaunchPad
and ss.enrolment_date &gt;= (CASE WHEN @FromEnrolmentDate IS NULL THEN dateadd(d, -56, getdate()) WHEN year(@FromEnrolmentDate) = 2000 THEN  dateadd(d, -56, getdate()) ELSE @FromEnrolmentDate END)
-- end of query --</CommandText>
